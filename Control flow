
x=float(input("enter the number: "))
if x>0:
    y=round(x)
    #print(y)
    if y>x:                                                                                                                   
        intPart=y-1
        #print(intPart)
    else:
        intPart=y
       # print(y)
else:
    y=round(x)
    if x>y:
        intPart=y+1
        #print(intPart)
    else:
        intPart=y
print(intPart)       # print(x)
if intPart%2==0:
    print("even")
else:
    print("odd")
